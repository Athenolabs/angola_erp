 "html": "<style>\n\t.print-format table, .print-format tr, \n\t.print-format td, .print-format div, .print-format p {\n\t\tfont-family: Monospace;\n\t\tline-height: 200%;\n\t\tvertical-align: middle;\n\t}\n\t@media screen {\n\t\t.print-format {\n\t\t\twidth: 4in;\n\t\t\tpadding: 0.25in;\n\t\t\tmin-height: 8in;\n\t\t}\n\t}\n</style>\n\n<p class=\"text-center\">\n\t{{ doc.company }}<br>\n\t{{ doc.select_print_heading or _(\"Invoice\") }}<br>\n</p>\n<p>\n\t<b>{{ _(\"Receipt No\") }}:</b> {{ doc.name }}<br>\n\t<b>{{ (\"Hora Entrada\") }}:</b> {{ doc.get_formatted(\"hora_entrada\") }}<br>\n\t<b>{{ (\"Hora Saida\") }}:</b> {{ doc.get_formatted(\"hora_saida\") }}<br>\n\t<b>{{ _(\"Customer\") }}:</b> {{ doc.nome_cliente }}\n</p>\n\n<hr>\n<table class=\"table table-condensed cart no-border\">\n\t<thead>\n\t\t<tr>\n\t\t\t<th width=\"50%\">{{ _(\"Item\") }}</b></th>\n\t\t\t<th width=\"25%\" class=\"text-right\">{{ _(\"Qty\") }}</th>\n\t\t\t<th width=\"25%\" class=\"text-right\">{{ _(\"Amount\") }}</th>\n\t\t</tr>\n\t</thead>\n\t<tbody>\n\t\t{%- for item in doc.servicos -%}\n\t\t<tr>\n\t\t\t<td>\n\t\t\t\t{{ item.servico_produto }}\n\t\t\t</td>\n\t\t\t<td class=\"text-right\">{{ item.quantidade }}<br>@ {{ item.get_formatted(\"preco_servico_produto\") }}</td>\n\t\t\t<td class=\"text-right\">{{ item.get_formatted(\"total_servico_produto\") }}</td>\n\t\t</tr>\n\t\t{%- endfor -%}\n\t</tbody>\n</table>\n<table class=\"table table-condensed no-border\">\n\t<tbody>\n\t\t<tr>\n\t\t\t<td class=\"text-right\" style=\"width: 70%\">\n\t\t\t\t{{ _(\"Net Total\") }}\n\t\t\t</td>\n\t\t\t<td class=\"text-right\">\n\t\t\t\t{{ doc.get_formatted(\"total_servicos\") }}\n\t\t\t</td>\n\t\t</tr>\n\n\t\t<tr>\n\t\t\t<td class=\"text-right\" style=\"width: 75%\">\n\t\t\t\t<b>{{ _(\"Grand Total\") }}</b>\n\t\t\t</td>\n\t\t\t<td class=\"text-right\">\n\t\t\t\t{{ doc.get_formatted(\"total_servicos\") }}\n\t\t\t</td>\n\t\t</tr>\n\t</tbody>\n</table>\n\n<hr>\n\n<p class=\"text-center\">{{ _(\"Thank you, please visit again.\") }}</p>", 
